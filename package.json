{
  "name": "storage-gateway",
  "version": "0.0.1",
  "description": "storage gateway",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\" --timeout 20000 --bail",
    "precoverage": "npm run pretest",
    "coverage": "nyc --reporter=html --reporter=text lb-mocha --allow-console-logs \"dist/__tests__\" --timeout 60000 --integration-tests",
    "preintegration-test": "npm run pretest",
    "integration-test": "lb-mocha --allow-console-logs \"dist/__tests__\" --timeout 60000 --integration-tests --dont-bail",
    "presome-test": "npm run pretest",
    "some-test": "lb-mocha --allow-console-logs \"dist/__tests__/zclean/*.js\" --timeout 45000 --integration-tests --no-bail",
    "dev-test": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/local/services\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t storage-gateway .",
    "docker:run": "docker run -p 3000:3000 -d storage-gateway",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "preopenapi-spec-export": "npm run build",
    "openapi-spec-export": "node ./dist/openapi-spec > dist/openapi-spec.json",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "enable-debug": "set DEBUG=loopback:connector*",
    "disable-debug": "set DEBUG=none",
    "pregenerate-spring-client": "npm run openapi-spec-export",
    "generate-spring-client": "node ./scripts/generate-spring-client.js",
    "pregenerate-node-client": "npm run openapi-spec-export",
    "generate-node-client": "node ./scripts/generate-node-client.js",
    "validate": "npm run clean && npm run build && npm run lint:fix && npm run test && npm run generate-spring-client && npm run generate-node-client",
    "check-circular": "npm run rebuild && npx madge --circular dist/index.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "Fabio Fenoglio <development@fabiofenoglio.it>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.0.3",
    "@loopback/boot": "^3.1.0",
    "@loopback/core": "^2.12.0",
    "@loopback/cron": "^0.5.0",
    "@loopback/health": "^0.8.1",
    "@loopback/logging": "^0.4.4",
    "@loopback/repository": "^3.2.0",
    "@loopback/rest": "^9.0.0",
    "@loopback/rest-explorer": "^3.0.3",
    "@loopback/service-proxy": "^3.0.3",
    "@microsoft/microsoft-graph-client": "^2.1.1",
    "@types/etag": "^1.8.0",
    "@types/fs-extra": "^9.0.4",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/sharp": "^0.26.1",
    "@types/uuid": "^8.3.0",
    "async-lock": "^1.3.0",
    "aws-sdk": "^2.923.0",
    "digest-stream": "^2.0.0",
    "etag": "^1.8.1",
    "etag-stream": "^1.1.6",
    "fresh": "^0.5.2",
    "fs-extra": "^9.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mysql": "^5.4.4",
    "minimist": "^1.2.5",
    "multer": "^1.4.2",
    "multistream": "^4.1.0",
    "range-stream": "^1.1.0",
    "rollbar": "^2.19.4",
    "sanitize-filename-ts": "^1.0.2",
    "sharp": "^0.27.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.6",
    "@loopback/eslint-config": "^10.0.2",
    "@loopback/testlab": "^3.2.8",
    "@microsoft/microsoft-graph-types": "^1.27.0",
    "@openapitools/openapi-generator-cli": "^2.1.15",
    "@types/async-lock": "^1.1.2",
    "@types/fresh": "^0.5.0",
    "@types/minimist": "^1.2.2",
    "@types/multer": "^1.4.4",
    "@types/multistream": "^2.1.1",
    "@types/node": "^10.17.44",
    "eslint": "^7.12.1",
    "husky": "^4.3.5",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.5"
  }
}
